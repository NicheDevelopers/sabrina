{
  "name": "sabrina",
  "version": "1.0.0",
  "description": "Discord music bot that plays audio from YouTube",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/main.ts",
    "start": "node --env-file=.env dist/main.js",
    "test": "jest",
    "docker-build": "VERSION=$(jq -r '.version' package.json) docker build --build-arg VERSION=$(jq -r '.version' package.json) -t sabrina:$(jq -r '.version' package.json) -t sabrina:latest .",
    "docker-up": "VERSION=$(jq -r '.version' package.json) docker-compose up -d --build",
    "docker-down": "docker-compose down",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts"
  },
  "keywords": [
    "discord",
    "music",
    "bot",
    "youtube"
  ],
  "author": "NicheDevs",
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.10.0",
    "@discordjs/voice": "^0.19.0",
    "@snazzah/davey": "^0.1.6",
    "discord.js": "^14.22.1",
    "dotenv": "^16.6.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0",
    "youtube-dl-exec": "^2.4.13",
    "yt-search": "^2.13.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.15.15",
    "@types/yt-search": "^2.10.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
